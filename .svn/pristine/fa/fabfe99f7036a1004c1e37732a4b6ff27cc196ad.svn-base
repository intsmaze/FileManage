@charset "utf-8";
/*************************学习掌上通--样式**************************/
body{ 
	font-size:12px;
	font-family: Tahoma, Helvetica, Arial, '宋体', sans-serif,; 
	color:#333;
	padding:0px;
	margin:0px;
	
}
/****top start*****/
.header{ height:86px; width:100%;}
.header_left{float:left; height:86px; background:url(../../images/nsfw/top_left_bg.jpg) left top no-repeat; width:500px;}
.header_left .h_logo{ margin:13px 0px 0px 20px;}
.header_right{float:right; height:86px; background:url(../../images/nsfw/top_right_bg.jpg) right top no-repeat; width:400px;}
.header_right .user_pro{float:right; padding:16px 20px 0px 0px;}
.header_right .user_pro h2{ color:#fff; font-size:12px;  line-height:22px;}
.header_right .h_nav{float:right; width:200px;}
.h_nav li{ float:left; width:32px; height:48px; margin:8px;}
.h_nav li a{ display:block; color:#fff; padding-top:32px; text-align:center; background:url(../../images/nsfw/e_icon.png) no-repeat;}
.h_nav li a.home{ background-position: 2px -112px;}
.h_nav li a.help{ background-position: -41px -112px;}
.h_nav li a.admin{ background-position: -84px -112px;}
.h_nav li a.exit{ background-position: -130px -112px;}
/****top end*****/

.main{ position:relative; height:auto; padding-left:152px;}
.main_left{ width:140px; padding:0px 6px;background:url(../../images/nsfw/top_left_bg.jpg) left -86px no-repeat; }
.m_menu{ border-radius:5px; border:1px solid #1f5597; background-color:#f2f2f2; width:140px;}
.m_menu dl{ padding:5px 0px;}
.m_menu dl dt{ line-height:25px; padding-left:35px; font-weight:bold; position:relative; height:30px; border-bottom:1px solid #d8dee3;}
.m_menu dl dt b{ width:15px; height:10px; display:block; position:absolute; left:8px; top:5px; background:url(../../images/nsfw/e_icon.png) 4px 5px no-repeat;}
.m_menu dl dd{ position:relative; padding-left:35px; line-height:33px; }
.m_menu dl dd b{ width:5px; height:5px; position:absolute; left:20px; top:14px; display:block;background:url(../../images/nsfw/e_icon.png) -53px 0px no-repeat;}
.m_menu dl dd a{ color:#333; }
.m_menu dl dd.curr b{ background-position:-29px 0px;}
.m_menu dl dd.curr a{ color:red;}

.m_content{
	padding:0px;
	width:990px;
	overflow: hidden;
	margin-right: 5px;
	background-color: #fff;
	background:rgba(255, 255, 255, 0.8) none repeat scroll 0 0 !important;/*实现FF背景透明，文字不透明*/
    filter:Alpha(opacity=80); background:#fff;/*实现IE背景透明*/
    position: relative;
}
.content_info{border:1px solid #ebeaea; width:742px; height:667px; }
.content_info .c_crumbs{ height: 35px; line-height:35px; position:relative; border-bottom:1px solid #bccfe2;}
.content_info .c_crumbs div{ position:relative; height:35px; padding-left:40px;  border-bottom:1px solid #64c864; display:inline-block;}
.content_info .c_crumbs b{ width:20px; height:20px; display:block; position:absolute; top:8px; left:10px; background:url(../../images/nsfw/e_icon.png) -72px 0px no-repeat;}
.content_info .c_crumbs .sear{ position:absolute; right:20px; top:6px; width:20px; height:20px; border:0px none; background:url(../../images/nsfw/e_icon.png) 0px -26px no-repeat; cursor:pointer;}
.content_info .search_art{ padding:5px 20px; height:22px; border-top:1px solid #e3e3e3; border-bottom:1px solid #e3e3e3; background-color:#fafafa; line-height:22px;}
.content_info .search_art li{ float:left; padding:0px 5px;}
.content_info .search_art .s_text{ border:1px solid #c7c7c7; height:20px; width:120px;}
.content_info .search_art .s_button{ background:url(../../images/nsfw/e_icon.png) -48px -27px no-repeat; width: 50px; height:24px; border:0px none; color:#fff;}
.content_info .search_art .s_sele{border:1px solid #c7c7c7; width:120px;}


.xzfwMain .content_info{width:960px; height:auto; border:0px none; background-color:transparent;}
.xzfwMain .content_info .c_crumbs{ height: 35px; font-size:14px; line-height:35px;position:relative; border-bottom:2px solid #bccfe2;}
.xzfwMain .content_info .c_crumbs div{font-size:14px;position:relative; height:35px; padding-left:40px;  border-bottom:2px solid #62adfe; padding-right:10px; display:inline-block; float:left;}

.xzfwMain .content_info .c_crumbs b{ width:20px; height:20px; display:block; position:absolute; top:8px; left:10px; background:url(../../images/nsfw/e_icon.png) -72px 0px no-repeat;}
.xzfwMain .content_info .c_crumbs .sear{ position:absolute; right:20px; top:6px; width:20px; height:20px; border:0px none; background:url(../../images/nsfw/e_icon.png) 0px -26px no-repeat; cursor:pointer;}
.xzfwMain .content_info .search_art{ padding:5px 10px; height:22px; border:1px solid #dbeafa; margin:5px 0px 10px 0px; background-color:#f0f7fe; line-height:22px;}
.xzfwMain .content_info .search_art li{ float:left; padding:0px 5px;}
.xzfwMain .content_info .search_art .s_text{ border:1px solid #c7c7c7; height:20px; width:120px;}
.xzfwMain .content_info .search_art .s_button{cursor:pointer; background:url(../../images/nsfw/e_icon.png) -48px -27px no-repeat; width: 50px; height:24px; border:0px none; color:#fff;}
.xzfwMain .content_info .search_art .s_sele{border:1px solid #c7c7c7; width:120px;}
.xzfwMain .content_info .search_art .s_excel{ background:url(../../images/nsfw/xzfwpt_bg.png) 0px -492px no-repeat; width: 71px; height:24px; border:0px none; color:#fff; cursor:pointer;}

.study_list{ }
.study_list li{ padding:5px; border-bottom:1px dashed #e4e4e4;}
.study_list li h2{ line-height:22px; height:22px; top:0px; margin-top:4px; left:0px; display:block;  margin-bottom:0px;}
.study_list li h2 a{  padding-left:5px;font-size:12px; color:#415779; float:left;}
.study_list li h2 b{ width:18px; height:18px; float:left; margin:3px;_margin-left:0px;background:url(../../images/nsfw/e_icon.png) -103px 0px no-repeat; }
.study_list li .yd{ line-height:33px; color:#646464; padding-left:8px;}
.study_list li .yd span{ color:red; margin-left:20px;}
.study_list li .date{ color:#a5a5a5; line-height:20px; padding-left:8px;}
.study_list li .oper{ text-align:right;}
.study_list li .oper input{ margin:0px 5px; border:0px none; padding:0px 5px 0px 15px; color:#466eb5; height:22px; background:url(../../images/nsfw/e_icon.png) no-repeat;}
.study_list li .oper input.xx{ background-position: -144px 2px ;}
.study_list li .oper input.ck{ background-position: -140px -34px ;}
.c_pate{ text-align:right; color:#666; padding: 5px 20px;}
.c_pate a{ margin:0px 4px; color:#333;}
.c_pate .p_text{ width:30px; height:20px; text-align:center; border:1px solid #d5d5d5; margin:0px 5px; }

.content_shor{ position:absolute; top:0px; right:0px; background-color:#; background:url(../../images/nsfw/right_bj.png) no-repeat bottom center; width:237px; height:667px; border:1px solid #ebeaea;}
.m_content .content_shor .shor_tit{height: 35px; line-height:35px; position:relative;  color:#666; border-bottom:1px solid #bccfe2;}
.content_shor .shor{ height:auto; padding-left:20px !important;   padding-top:30px;
	margin:0px; }
	.m_content .content_shor .shor_tit div{position:relative; height:35px; padding-left:10px;  border-bottom:1px solid #64c864; display:inline-block;}
.content_shor .shor li{ float:left; width:79px; height:92px;  margin:0px 5px; padding-bottom:5px;}
.content_shor .shor li a{ color:#333; font-size:14px; text-align:center; display:block; padding-top:15px;}
.content_shor .shor li a:hover{ width:79px; height:92px; background:url(../../images/nsfw/kjfx_hover.png) no-repeat;}
.content_shor .shor li a p{ margin-top:2px; line-height:22px;}
.t_list{text-align:center;}
.t_list table tr td{ padding:0px 4px;border:1px solid #dbeafa; height:35px; color:#666;}
.t_list table .t_tit{ background-color:#e3f1ff; border-top:1px solid #fbfbfb; font-weight:bold;}
.t_list table .t_tit:hover{ background-color:#e3f1ff;}
.t_list table .t_tit td{color:#616161; }
.t_list table tr{}
.t_list table tr:hover{ background-color:#dff0ff;}
.t_list table tr:hover td{ color:#333; font-size:12px;}
.xzfwMain .t_list table tr.trBg{   background-color:rgba(227,241,255,0.5); filter:alpha(opacity=50); }
.xzfwMain .t_list table tr.trBg:hover{   background-color:rgba(227,241,255,1);}

/****考试信息start*****/
.exam_window{ margin:20px auto; border:5px solid rgba(0,0,0,0.5); border-radius:5px; width:645px; background-color:#fff;}
.exam_window .ew_top{ background:url(../../images/nsfw/exam_bg.png) 0px 0px no-repeat; height:50px; }
.exam_window .ew_top h2{ font:24px/50px "微软雅黑"; text-shadow:0px 0px 4px rgba(0,0,0,0.5); margin-left:20px; color:#fff;}
.exam_window .ew_top input.b_tit{ border:0px none; height:27px; width:78px; background:url(../../images/nsfw/exam_bg.png) -568px -158px no-repeat; margin:10px 10px 0px 0px; cursor:pointer; font-size:14px; color:#1c5171;}
