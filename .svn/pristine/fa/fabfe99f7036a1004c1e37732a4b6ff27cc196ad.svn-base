.ew_tit{ padding:30px; text-align:center;}
.ew_tit h2{ font:24px/33px "微软雅黑"; margin-bottom:10px;}
.ew_tit p{ font-size:14px; color:#666;}
.s_exam .se_item{ border:2px solid #dcdcdc; border-radius:6px; padding:20px;margin:10px 100px; position:relative;}
.s_exam .se_item dl{ padding:20px 50px;}
