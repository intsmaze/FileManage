/****开始考试start*****/
.ex_logo{ width:700px; padding-left:38px; padding-top:10px; height:60px;}
.s_exam{ position:relative; width:100%;}
.s_exam b{ position:absolute; width:9px; height:480px; background:url(../../images/nsfw/ex_br.png) no-repeat;}
