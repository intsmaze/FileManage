.answer_item .card li{ border-bottom:1px solid #F5F5F5; float:left; border-right:1px solid #F5F5F5; width:39px; height:39px; background:url(../../images/nsfw/exam_bg.png) -218px -188px no-repeat;}
.answer_item .card li a{ display:block; height:39px; line-height:39px; text-align:center; }
.answer_item .card li.c_not{ background-position: -218px -188px;}
.answer_item .card li.c_not a{ color:#666;}
.answer_item .card li.c_have{ background-position: -260px -188px;}
.answer_item .card li.c_have a{ color:#fff;}
