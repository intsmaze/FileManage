.s_exam_top .SEdate span{ font:20px/45px "微软雅黑"; color:#555;}
.s_exam_top .SEdate input{ position:absolute; border:0px none; right:5px; top:5px; width:30px; height:30px; background:url(../../images/nsfw/exam_bg.png) no-repeat; font-size:0px; line-height:0px;}
