.ex_info li .ex_name{float:left; width:88px; padding-left:40px; background-color:#f5f5f5; position:relative; height:46px;line-height:46px;}
.ex_info li .fl{line-height:46px; padding-left:10px;}
.ex_info li .fl span{color:#999;}
.ex_info li .ex_name b{ position:absolute; width:20px; height:20px; left:10px; top:13px; background:url(../../images/nsfw/exam_bg.png) no-repeat;}
.ex_info li.ksr .ex_name b{ background-position: -13px -103px;}
.ex_info li.kjrx .ex_name b{ background-position: -13px -151px;}
.ex_info li.kksj .ex_name b{ background-position: -13px -198px;}
.ex_info li.wcsj .ex_name b{ background-position: -13px -248px;}
.ex_info li.ssbm .ex_name b{ background-position: -53px -104px;}
.ex_info li.kssx .ex_name b{ background-position: -53px -152px;}
.ex_info li.yysc .ex_name b{ background-position: -53px -199px;}
.ex_info li.sysc .ex_name b{ background-position: -53px -249px;}

.ewam_button{ height:50px; margin:10px 0px;text-align:center;}
.ewam_button input{ border:0px none; background:url(../../images/nsfw/exam_bg.png) no-repeat; width:92px; cursor:pointer; height:32px; margin:0px 5px; font-size:14px; color:#fff; font-weight:bold; text-shadow:0px 0px 4px rgba(0,0,0,0.5); }
.ewam_button input.b_green{ background-position: 0px -54px;}
.ewam_button input.b_blue{ background-position: -100px -55px;}

.ew_main .file_text{ border:1px solid #ddd; border-radius:5px; padding:10px;overflow-y:auto}
.ew_main .file_text h2{ font-size:16px; color:#000; margin-bottom:5px;}
.ew_main .file_text p{ text-indent:2em; line-height:22px; color:#444;}
.ew_main .file_text a{ text-decoration:underline; color:blue;}
