body { margin:0;}
img{border: medium none;}
#home_top { margin:auto;height:35px; background-image:url(../images/head/top_bg.gif); font-size:12px;}
#home_top .top_nav { margin:auto;width:960px; height:35px; font-size:12px;}
#home_top .top_nav .left { width:356px; float:left; text-align:left; color:#696969; margin-top:5px;}
#home_top .top_nav .left a{ padding:2px;color:#696969;text-decoration: none;}
#home_top .top_nav .left a:hover{ text-decoration:none;background:#4A4A4A; color:#FFFFFF;}
#home_top .top_nav .left li { float:left;}

#home_top .top_nav .right { float:right; color:#696969; text-align:right;}

#home_top .top_nav .right .login {  color:#696969;text-align:right; margin-right:6px; float:right; padding:2px;}
#home_top .top_nav .right .login a{ color:#005FAB;text-decoration: none; padding:2px; margin-left:2px; margin-right:2px; margin-top:5px; text-decoration: underline; text-align:right;}
#home_top .top_nav .right .login a:hover{ padding:2px;text-decoration:none;background:#4A4A4A; color:#FFF;}

#home_top .top_nav .right .login2 { color:#696969;text-align:right; float:right; margin-right:6px; padding:2px; }
#home_top .top_nav .right .login2 a{ color:#005FAB;text-decoration: none; padding:2px; margin-left:2px; margin-right:2px;text-decoration: underline;}
#home_top .top_nav .right .login2 a:hover{ padding:2px;text-decoration:none;background:#4A4A4A; color:#FFF;}

#home_top .top_nav .right .more { float:right;padding:8px 0 3px 0; color:#696969;text-decoration: none;}
#home_top .top_nav .right .more a{ padding:2px;color:#696969;text-decoration: none;}
#home_top .top_nav .right .more a:hover{ padding:2px;text-decoration:none;background:#4A4A4A; color:#FFF;}







