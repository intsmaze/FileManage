/*分页css*/
.Pagination { background-color:#ECF5F9; padding:16px 14px 10px 0; text-align:right;}
.Pagination a{color:#0B70B5; text-decoration:none; padding-right:14px; border:1px solid #B1CEEB; padding:5px 8px 3px 8px; background-color:#FBFBFB; margin:0 2px;}
.Pagination a:hover{text-decoration:none; color:#c00; background-color:#E8EEF4;}
.Pagination em{background-color:#FBFBFB;
border:1px solid #D5D5D5;
color:#666666;
margin:0 2px;
padding:5px 8px 3px;
text-decoration:none;}
.Pagination strong{background-color:#E8EEF4;
border:1px solid #B1CEEB;
color:#CC0000;
font-weight:bold;
margin:0 2px;
padding:5px 8px 3px;
text-decoration:none;}
