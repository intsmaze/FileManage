/* 全局CSS定义 */
body{font-family:宋体;margin:0 auto;padding:0;background:#FFF;font-size:12px;color:#444;}
body > div{margin-right:auto;margin-left:auto;}
div,form,ul,ol,li,span,p{margin:0;padding:0;border:0;}
dl,dt,dd{margin:0;padding:0;border:0;}
img,a img{border:0;margin:0;padding:0;}
h1,h2,h3,h4,h5,h6{margin:0;padding:0;font-weight:normal;}
ul,ol,li{list-style:none}
table,td,input{font-size:12px}
h1 { display:none;}
.dle { clear:both; font-size:0;}
.banner_head { width:960px; margin:0 auto 8px auto;display:block;}

/* 默认链接颜色 */
a{ outline-style:none; color:#444; text-decoration:none}
a:hover{ color:#c00; text-decoration:underline;}


/* HEAD样式 */
#head { width:960px; margin:0 auto 8px auto;font-family:宋体;}
#head .mininav { height:19px; background:url(../images/mininav_bg.gif); padding:16px 10px 0 0; text-align:right; color:#BBB;}
#head .mininav a{padding:2px 3px 0 3px; color:#696969;text-decoration: none;}
#head .mininav a:hover{text-decoration:underline; color:#c00;}
#head .subNav { height:94px; background:url(../images/subNav_bg.gif) no-repeat right;}
#head .subNav .logo { width:188px; height:58px; float:left; padding:19px 0 0 15px;}
#head .subNav .daohang { width:345px; height:30px; float:right; padding:26px 10px 0 0; color:#3B6C00;}
#head .subNav .daohang span { padding-right:12px;}
#head .subNav .daohang a{padding:0 5px; color:#3B6C00;text-decoration: none;}
#head .subNav .daohang a:hover{text-decoration:underline; color:#c00;}
#head .nav { height:50px; background:url(../images/Nav_bg.gif); cursor:pointer;text-align:center; font-size:14px; font-weight:bold; padding-left:100px;}
#head .nav a{ width:110px; height:31px; display:block; float:left; padding-top:19px; color:#FBFBFB;text-decoration: none; text-align:center;}
#head .nav a:hover{text-decoration:none; color:#0062A2;cursor:pointer; background:url(../images/Nav_bg2.png);_background:none;
		_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src="/images/Nav_bg2.png",sizingMethod="scale")}
#head .nav .na1 { width:110px; height:31px;padding-top:19px;color:#0062A2;cursor:pointer; background:url(../images/Nav_bg2.png);_background:none;
		_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src="/images/Nav_bg2.png",sizingMethod="scale")}

#head .nav .upload {cursor:pointer;display:block;height:36px;right:16px;width:126px; float:right; padding-top:7px; margin-right:10px;}

#head .tag { height:23px; background:url(../images/Nav_bg3.gif); padding:17px 12px 0 12px;}
#head .tag dl { width:930px; height:14px;}
#head .tag dt { width:560px; height:14px; float:left;text-align:left;}
#head .tag dt a{ color:#444;text-decoration: underline;}
#head .tag dt a:hover{text-decoration:underline; color:#c00;}
#head .tag dd { width:360px; height:14px; color:#3B6C00; float:right; text-align:right;}
#head .tag dd a{ color:#3B6C00;text-decoration: none;}
#head .tag dd a:hover{text-decoration:underline; color:#c00;}


